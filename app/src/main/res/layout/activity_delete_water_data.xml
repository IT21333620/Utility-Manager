<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.delete_water_data">

    <Button
        android:id="@+id/button40"
        android:layout_width="143dp"
        android:layout_height="@dimen/btn_height"
        android:layout_marginStart="120dp"
        android:layout_marginBottom="280dp"
        android:text="@string/delete"
        app:backgroundTint="@color/orange"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName17"
        android:layout_width="342dp"
        android:layout_height="58dp"
        android:layout_marginTop="416dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:hint="    Enter Week"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        android:textColorHint="@color/material_dynamic_neutral_variant30"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/circle34"
        android:layout_width="212dp"
        android:layout_height="212dp"
        android:layout_marginEnd="292dp"
        android:layout_marginBottom="632dp"
        android:background="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/circle35"
        android:layout_width="235dp"
        android:layout_height="235dp"
        android:layout_marginStart="284dp"
        android:layout_marginBottom="612dp"
        android:background="@drawable/circle2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/elec_manager10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="292dp"
        android:layout_marginEnd="92dp"
        android:text="@string/delete_water_data"
        android:textColor="@color/orange"
        android:textSize="@dimen/title2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>