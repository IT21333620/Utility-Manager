    <TextView
        android:id="@+id/sqr2"
        android:layout_width="227dp"
        android:layout_height="201dp"
        android:layout_marginTop="432dp"
        android:layout_marginEnd="92dp"
        android:width="150dp"
        android:height="150dp"
        android:background="@drawable/squre1"
