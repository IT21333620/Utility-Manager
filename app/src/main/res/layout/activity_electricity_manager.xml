        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cir"
        android:layout_width="117dp"
        android:layout_height="115dp"
        android:layout_marginTop="396dp"
        android:layout_marginEnd="236dp"
